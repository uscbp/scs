		<comment>Module ThFEFmem - Part of the Thalamus</comment>
		<methods>public void initModule()
 {
    THNewActivation= (NslDouble2)nslGetValue("crowleyModel.thal1.THNewActivation") ;
  }

public void initRun(){

    ThFEFmem_out = 0;
    Thfefmem = 0;
    Thfefmemtm=0.006;
    ThfefmemK1=1.5;
    ThfefmemK2=0.5;
    ThfefmemK3=0.5;
    ThFEFmemsigma1=30;
    ThFEFmemsigma2=65;
    ThFEFmemsigma3=0;
    ThFEFmemsigma4=60;
  }

public void simRun(){
  /* &lt;Q&gt; FEFmem_in  SNRmedburst_in  ThMEDlcn_in  THNewActivation*/
    THNewActivation= (NslDouble2)nslGetValue("crowleyModel.thal1.THNewActivation") ;

  // System.err.println("@@@@ ThFEFmem simRun entered @@@@");
    Thfefmem=nslDiff(Thfefmem,Thfefmemtm,-Thfefmem
                     +(ThfefmemK1*FEFmem_in)
                     -(ThfefmemK2*SNRmedburst_in)
                     -(ThfefmemK3*ThMEDlcn_in)
                     +THNewActivation);


   ThFEFmem_out=Nsl2Sigmoid.eval(Thfefmem,ThFEFmemsigma1,
                            ThFEFmemsigma2,
                            ThFEFmemsigma3,
                            ThFEFmemsigma4);

	// 96/12/20
	//System.out.println("ThFEFmem_out: " + ThFEFmem_out);
  }






