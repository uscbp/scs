		<implements></implements>
		<comment>Here is the class representing the superior colliculus burst neuron
saccade generating (SCsac) layer.</comment>
		<methods>public void initModule()
{
	FOVEAX = (NslInt0)nslGetValue("crowleyModel.FOVEAX");
	FOVEAY = (NslInt0)nslGetValue("crowleyModel.FOVEAY");  
}

public void initRun()
{
	scsac =0;
	SCsac_out = 0;
	//threshold to the saccade
	SCsacsigma1 =   30;
	SCsacsigma2 =  160;
	SCsacsigma3 =    0;
	//LNK_SC2_1
	SCsacsigma4 = 1000;
	//of the matrix SCsac.

	SCsacvelK   = 0.9 / ( SCsac_out.getSize1() * SCsacsigma4 );

	SCsacmax            =    0;
	SCsacprevmax        =    0;
	scsactm =      0.01;
	BSGscsacK           =    0.06;
	SCfefsacK =    1;
	SCsnrlbK =     2;
	SCscbuK =     18.0;

	SCscqvK= 1.0;
}

public void simRun()
{
	// System.err.println("@@@@ SCsac simRun entered @@@@");
	scsac=nslDiff(scsac,scsactm, -scsac + ( SCscqvK * SCqv_in ) + ( SCfefsacK * FEFsac_in ) 
                 - ( SCsnrlbK * SNRlatburst_in ) - ( SCscbuK * SCbu_in[4][4]));
	SCsac_out=Nsl2Sigmoid.eval(scsac,SCsacsigma1,SCsacsigma2,SCsacsigma3,SCsacsigma4);
}





</methods>
	</nslm>
</module>
