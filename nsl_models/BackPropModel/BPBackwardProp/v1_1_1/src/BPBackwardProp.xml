		<comment></comment>
		<methods>public void initModule() 
{
	lrate.nslSetAccess('W');
	lrate=0.8f;
}

public void simTrain() 
{
	for(int i=0; i&lt;hidSize; i++)
	{
		for(int j=0; j&lt;outSize; j++)
		{
			delta[j]=mf[j]*(1.0f-mf[j])*bInput[j];
			dh[j]=lrate*delta[j];
			dw[i][j]=lrate*delta[j]*fInput[i];
		}
		bOutput[i] = nslProd(w[i], delta);
	}
}



</methods>
