		<comment></comment>
		<methods>public void initModule() 
{
	lrate.nslSetAccess('W');
	lrate=0.8f;
}

public void simTrain() 
{
	backwardPass();
}

public void backwardPass()
{
	for(int i=0; i&lt;hidSize; i++)
	{
		for(int j=0; j&lt;inSize; j++)
		{
			delta[i]=mf[i] * (1.0f-mf[i]) * bInput[i];
			dh[i]=lrate*delta[i];
			dw[j][i]=dh[i]*fInput[j];
		}
	}
}



