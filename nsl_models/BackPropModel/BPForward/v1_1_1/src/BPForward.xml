		<comment></comment>
		<methods>public void initModule() 
{
	for(int i=0; i&lt;hidSize; i++)
	{
		h[i]=nslRandom(-0.5f,0.5f);
		for(int j=0; j&lt;inSize; j++)
		{
			w[j][i]=nslRandom(-0.5f,0.5f);
		}
	}
	dw = 0.0;
	dh = 0.0;
}

public void forward()
{
	mp = fInput*w;
	mf = nslSigmoid(mp + h);
}

public void simTrain() 
{
	w = w + dw;
	h = h + dh;
	forward();
}

public void simRun()
{
	forward();
}



</methods>
