		<methods>public void initModule()
{
	fcx_out=0.;
	fcx_noise=(NslDouble0)nslGetValue("dartModel.fcx_noise");
}
   
public void simTrain() 
{
	simRun();
}  
 
public void simRun()
{
	int i,j;
	NslDouble0 mx(), my(), dx(), dy();
     
	mx = 1. + 9.*p_in/50.;
	my = 1.5;
     
	for(i=0;i&lt;10;i++)
	{
		dx = mx-i;
		for(j=0;j&lt;4;j++)
		{
			dy = my - j;
			fcx_out[i][j] = fcx_noise * nslRandom() + (1.-fcx_noise)*nslExp(-1.*(dx*dx/sx2 + dy*dy/sy2));
		}
	}
}


