		<methods>public void initModule() 
{
	io_out = 2.;
}

public void simTrain() 
{
	simRun();
}

public void simRun() 
{
	double nuc_act;

	nuc_act = nslSumRows(nuc_in);
	io_mp   = sens_in - .01*nuc_act;
	io_out  = f_max * nslSigmoid(io_mp,slope,offset);
}


