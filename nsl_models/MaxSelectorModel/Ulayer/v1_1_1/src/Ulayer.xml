		<methods>public void initModule() 
{
	hu.nslSetAccess('W');
	uf=0;
	up=0;
	tau = 1.0;
	w1= 1.0;
	w2= 1.0;
	hu= 0.1;
	k= 0.1;
}

public void simRun() 
{
	up = nslDiff(up, tau, -up + w1*uf-w2*v_in - hu + s_in);
	uf = nslStep(up,k,0,1.0);
}



